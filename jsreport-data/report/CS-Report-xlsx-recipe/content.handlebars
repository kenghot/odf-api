{{#xlsxAdd "xl/worksheets/sheet1.xml" "worksheet.sheetData[0].row"}}
<row r="1">
    <c r="A1" t="inlineStr"><is><t>{{checkEmpty title1}}</t></is></c>
</row>
{{/xlsxAdd}}
{{#xlsxAdd "xl/worksheets/sheet1.xml" "worksheet.sheetData[0].row"}}
<row r="2">
    <c r="A2" t="inlineStr"><is><t>{{checkEmpty title2}}</t></is></c>
</row>
{{/xlsxAdd}}
{{#xlsxAdd "xl/worksheets/sheet1.xml" "worksheet.sheetData[0].row"}}
<row r="3">
    <c r="A3" t="inlineStr"><is><t>{{paymentTime}}</t></is></c>
</row>
{{/xlsxAdd}}

{{#xlsxAdd "xl/worksheets/sheet1.xml" "worksheet.sheetData[0].row"}}
<row r="4">
    <c r="A4" t="inlineStr"><is><t>ลำดับ</t></is></c>
    <c r="B4" t="inlineStr"><is><t>วันเดือนปีที่ชำระเงิน</t></is></c>
    <c r="C4" t="inlineStr"><is><t>ชื่อ - สกุล</t></is></c>
    <c r="D4" t="inlineStr"><is><t>เลขบัตรประชาชน</t></is></c>
    <c r="E4" t="inlineStr"><is><t>รหัสผู้กู้</t></is></c>
    <c r="F4" t="inlineStr"><is><t>เลขที่สัญญา</t></is></c>
    <c r="G4" t="inlineStr"><is><t>ปีสัญญา</t></is></c>
    <c r="H4" t="inlineStr"><is><t>รหัสจังหวัด</t></is></c>
    <c r="I4" t="inlineStr"><is><t>ลูกหนี้จังหวัด</t></is></c>
    <c r="J4" t="inlineStr"><is><t>พื้นที่ที่ชำระเงิน</t></is></c>
    <c r="K4" t="inlineStr"><is><t>รหัสสาขา</t></is></c>
    <c r="L4" t="inlineStr"><is><t>เลขที่ใบเสร็จ</t></is></c>
    <c r="M4" t="inlineStr"><is><t>รหัสเครื่อง</t></is></c>
    <c r="N4" t="inlineStr"><is><t>จำนวนเงิน</t></is></c>
</row>
{{/xlsxAdd}}

{{#each data}}
    {{#xlsxAdd "xl/worksheets/sheet1.xml" "worksheet.sheetData[0].row"}}
    <row>
        <c t="inlineStr" s="1"><is><t>{{getOrderNumber @index}}</t></is></c>
        <c t="inlineStr" s="1"><is><t>{{formatThaiDate paidDate}}</t></is></c>
        <c t="inlineStr" s="1"><is><t>{{checkEmpty name}}</t></is></c>
        <c t="inlineStr" s="1"><is><t>{{checkEmpty reference1}}</t></is></c>
        <c t="inlineStr" s="1"><is><t>{{checkEmpty reference2}}</t></is></c>
        <c t="inlineStr" s="1"><is><t>{{checkEmpty documentNumber}}</t></is></c>
        <c t="inlineStr" s="1"><is><t>{{checkEmpty fiscalYear}}</t></is></c>
        <c t="inlineStr" s="1"><is><t>{{checkEmpty orgCode}}</t></is></c>
        <c t="inlineStr" s="1"><is><t>{{checkEmpty addressProvince}}</t></is></c>
        <c t="inlineStr" s="1"><is><t>{{checkEmpty paymentAreaType}}</t></is></c>
        <c t="inlineStr" s="1"><is><t>{{checkEmpty counterNo}}</t></is></c>
        <c t="inlineStr" s="1"><is><t>{{checkEmpty serviceRunNo}}</t></is></c>
        <c t="inlineStr" s="1"><is><t>{{checkEmpty termNo}}</t></is></c>
        <c t="inlineStr" s="1"><is><t>{{currency amount true}}</t></is></c>
    </row>
    
    {{/xlsxAdd}}
{{/each}}

{{{xlsxPrint}}}